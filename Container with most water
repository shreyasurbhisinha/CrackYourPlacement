class Solution {
public:
    int maxArea(vector<int>& height) {
        int n=height.size();
        int i=0;
        int j=n-1;
        int maxarea=INT_MIN;
        int val=INT_MAX;
        while(i<j){
            val=min(height[i],height[j]);
            maxarea=max(maxarea,val*(j-i));
            if(height[i]<height[j]){
                i++;
            }
            else{
                j--;
            }

        }
        return maxarea;
        
    }
};
