class Solution {
public:
    int getDecimalValue(ListNode* head) {
        int ans=0;
        int cnt=0;
        ListNode* temp=head;
        while(temp){
            temp=temp->next;
            cnt++;
        }
        temp=head;
        while(temp){
            ans+=(temp->val)*pow(2,cnt-1);
            cnt--;
            temp=temp->next;

        }
        return ans;
        
    }
};
