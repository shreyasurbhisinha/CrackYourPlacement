class Solution {
public:
    int rangeSumBST(TreeNode* root, int low, int high) {
         sum=0;
        preorder(root,low,high);
        return sum;
        
    }
    private:
    int sum;
     void preorder(TreeNode* & root,int& low,int & high){
        int sum;
         
        if(root==nullptr){
            return;
        }
            if(root->val>=low && root->val<=high){
                sum+=root->val;
            }
            preorder(root->left,low,high);
            preorder(root->right,low,high);
            

        
    }
};
