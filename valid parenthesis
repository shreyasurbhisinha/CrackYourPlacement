class Solution {
public:
    bool isValid(string s) {
        stack<char> st;
        st.push('a');
        for(auto a:s){
           
             if(a=='(' || a=='{' || a=='['){
                st.push(a);
            }
            else if(a==')' && st.top()=='('){
                st.pop();
               
            }
             else if(a=='}' && st.top()=='{'){
                st.pop();
               
            }
             else if(a==']' && st.top()=='['){
                st.pop();
               
            }
            else{
                return false;
            }
           
        }
        if(st.size()==1){
            return true;
        }
        else{
            return false;
        }

        
    }
};
